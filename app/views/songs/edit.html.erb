<h1> Update a Song! </h1>

<%= form_for(@song) do |f| %>
    <%= f.label "Name: " %>
    <%= f.text_field :name %>
    <br>
    <%= f.label "Artist_id: " %>
    <%= f.text_field :artist_id %>
    <br>
    <%= f.label "Genre_id: " %>
    <%= f.text_field :genre_id %>
    <br>
    <%= f.submit "Update Song" %>

<% end %>